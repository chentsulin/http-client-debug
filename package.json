{
  "name": "http-client-debug",
  "version": "0.1.0",
  "description": "debug utils for http-client",
  "license": "MIT",
  "repository": "chentsulin/http-client-debug",
  "author": {
    "name": "C.T. Lin",
    "email": "chentsulin@gmail.com",
    "url": "github.com/chentsulin/http-client-debug"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "files": [
    "index.js",
    "lib/"
  ],
  "keywords": [
    "http",
    "http-client",
    "fetch",
    "isomorphic-fetch",
    "debug"
  ],
  "dependencies": {
    "http-client": "^4.0.1",
    "lodash.get": "^4.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.8.0",
    "chai": "^3.5.0",
    "eslint": "^2.10.2",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.8.0",
    "isomorphic-fetch": "^2.2.1",
    "mocha": "^2.4.5",
    "nyc": "^6.4.4",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.4"
  },
  "options": {
    "mocha": "--compilers js:babel-register --require babel-polyfill ./**/__tests__/**/*.spec.js"
  },
  "scripts": {
    "clean": "rimraf lib dist .nyc_output",
    "build": "npm run clean && babel src -d lib --ignore __tests__",
    "testonly": "NODE_ENV=test mocha $npm_package_options_mocha",
    "test": "npm run lint && npm run testonly",
    "test:cov": "nyc npm test",
    "lint": "eslint .",
    "preversion": "npm test",
    "prepublish": "npm run clean && npm run build"
  },
  "devEngines": {
    "node": "4.x || 5.x || 6.x",
    "npm": "2.x || 3.x"
  }
}
